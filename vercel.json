{
  "buildCommand": "pnpm build",
  "installCommand": "pnpm install --frozen-lockfile",
  "git": {
    "deploymentEnabled": {
      "main": true
    }
  },
  "redirects": [
    {
      "source": "/index.html",
      "destination": "/",
      "permanent": true
    },
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "elevateforhumanity.org"
        }
      ],
      "destination": "https://elevateforhumanity.institute/:path*",
      "permanent": true
    },
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "www.elevateforhumanity.org"
        }
      ],
      "destination": "https://elevateforhumanity.institute/:path*",
      "permanent": true
    }
  ]
}
